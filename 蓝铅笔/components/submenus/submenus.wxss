/* submenu */
.submenus {
  width: 750rpx;
  height: 330rpx;
  background-color: white;
  padding: 20rpx 0 4rpx 0;
  margin-bottom: 20rpx;
}
.submenus-list {
  display: flex;
  flex-wrap: wrap;
}
.submenus-item {
  width: 184rpx;
  padding-bottom: 16rpx;
}
.submenus-navTo {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.submenus-icon {
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  margin-bottom: 8rpx;
}
.submenus-item .text {
  color: #222;
  font-size: 28rpx;
  font-weight: 400;
  text-align: center;
}
